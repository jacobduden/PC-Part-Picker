{
  "name": "PC-Part-Picker",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "proxy": "https://api.igdb.com/v4/games",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "cd server && npm start",
    "start:dev": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "heroku-postbuild": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacobduden/PC-Part-Picker.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jacobduden/PC-Part-Picker/issues"
  },
  "homepage": "https://github.com/jacobduden/PC-Part-Picker#readme",
  "dependencies": {
    "apollo-server-express": "^2.25.2",
    "build": "^0.1.4",
    "concurrently": "^6.2.0",
    "cors": "^2.8.5",
    "cors-anywhere": "^0.4.4",
    "dotenv": "^10.0.0",
    "graphql": "^15.5.1",
    "graphql-playground": "^1.3.17",
    "http-proxy-middleware": "^2.0.1",
    "if-env": "^1.0.4",
    "react-bootstrap": "^1.6.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "request": "^2.88.2",
    "watch": "^1.0.2"
  }
}
